            <Button fx:id="btnRegresar" layoutX="263.0" layoutY="164.0" mnemonicParsing="false" onAction="#regresarCrearProyecto" prefHeight="26.0" prefWidth="98.0" style="-fx-background-color: #E84F31; -fx-border-radius: 7px; -fx-font-weight: bold;" text="Regresar" textFill="WHITE" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
